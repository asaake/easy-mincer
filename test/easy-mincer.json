{
  "port": 3000,
  "serverRoot": "/assets",
  "targets": ["main.js", "main.css"],
  "paths": [
    "app/assets/javascripts",
    "app/assets/stylesheets",
    "app/assets/templates"
  ],
  "publicPaths": [
    "app/public/javascripts",
    "app/public/stylesheets",
    "app/public/templates"
  ],
  "rewrites": {
    "/**": "/public.html"
  },
  "log": {
    "debug": false,
    "info": false,
    "warn": true,
    "error": true
  },
  "useSourceMaps": false,
  "useJsCompressor": false,
  "useCssCompressor": false
}